    "ax[1].set(xlabel=\"100 most frequent words\", ylabel=\"Probability\")\n",
    "\n",
    "# change y-scale to logarithmic scale\n",
    "ax[0].set_yscale(\"log\")\n",
    "ax[1].set_yscale(\"log\")\n",
    "plt.show()"
   ]
